<!DOCTYPE html>
<html>
  <head>
    <title>Housecontrol</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'shared/header' if user_signed_in?  %>
    <div class="container">
      <div class="main">
        <%= yield %>
      </div>
    </div>
  </body>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</html>
